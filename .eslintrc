{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "jest": true
  },
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-return-assign": 0,
    "no-multi-spaces": ["error", { "exceptions": { "ImportDeclaration": true, "Property": true } }],
    "camelcase": 0,
    "template-curly-spacing" : "off",
    "indent": ["error", 2, {
      "ignoredNodes": ["TemplateLiteral"]
    }]
  }
  // "globals": {
  //   "google": 1,
  //   "NProgress": 1,
  //   "jest": 1
  // }
}
